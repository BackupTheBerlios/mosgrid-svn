	<cml:entry id="grompp.NumberOfSteps" term="nsteps" title="Number of Steps" dataType="xsd:integer" unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
				The number of steps the simulation should at most perform.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
				The number of steps the simulation should at most perform.
			</xhtml:p>
		</cml:description>
	</cml:entry>

	<cml:entry id="grompp.MinimizerAlgorithm" term="integrator" title="Minimizer Algorithm" dataType="xsd:string"  unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
				Algorithm that is used to minimize the system.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
				A system, that has an initial configuration, can contain clashes
				between atoms and by that lead to an instable MD run. With this
				cml:entry one specifies the algorithm that is used to minimize the
				system.
			</xhtml:p>
		</cml:description>
		<cmlx:restriction id="Min.SteepestDescent" title="Steepest Descent">steep</cmlx:restriction>
		<cmlx:restriction id="Min.ConjugateGradient" title="Conjugate Gradient">cg</cmlx:restriction>
		<cmlx:restriction id="Min.lBFGS" title="quasi-Newtonian algorithm low-memory Broyden-Fletcher-Goldfarb-Shanno">l-bfgs</cmlx:restriction>
	</cml:entry>

	<cml:entry  id="grompp.Min.Tolerance"  term="emtol" title="Tolerance of accuracy." dataType="xsd:double"
		unitType="unitType:force" units="nonSi:kJ.mol-1nm-1">
		<cml:definition>
			<xhtml:p>
				The value of maximal force up to which the minimization process should
				be converged.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
				In a minimization process contacts and clashes between neighbouring
				atoms will be removed. If a system is sufficiently minimized will be
				determined by the maximal force a collision between two atoms
				produces.
			</xhtml:p>
		</cml:description>
	</cml:entry>

	<cml:entry id="grompp.Min.Stepsize" term="emstep" title="Size for Minimization Step" dataType="xsd:double"
		unitType="unitType:length" units="nonSi:nanometers">
		<cml:definition>
			<xhtml:p>
				Length an atom can be moved in a minimization step.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
				Amount of length an atom should be moved in a minimization step.
			</xhtml:p>
		</cml:description>
	</cml:entry>

	<cml:entry id="grompp.IntegratorAlgorithm" term="integrator" title="Integrator Algorithm" dataType="xsd:string" unitType="unitType:none" units="si:none">
	  <cml:definition>
	    <xhtml:p>
	      Algorithm that is used to integrate the Newtons equations of dynamics.
	    </xhtml:p>
	  </cml:definition>
	  <cml:description>
	    <xhtml:p>
	      Every step in a MD simulation has requires the integration of Newtons equations of movement. For this to accomplish different algorithms can be used. With that cml:entry one can specify that algorithm.
	    </xhtml:p>
	  </cml:description>
	  <cmlx:restriction id="Int.LF" title="Leap Frog">md</cmlx:restriction>
	  <cmlx:restriction id="Int.LF4G" title="Leap Frog 4 Gaussian Numbers">sd</cmlx:restriction>
	  <cmlx:restriction id="Int.LF1G" title="Leap Frog 1 Gaussian Number">sd1</cmlx:restriction>
	  <cmlx:restriction id="Int.Euler" title="Euler">bd</cmlx:restriction>
	</cml:entry>
 
	<cml:entry id="grompp.Int.Timestep" term="dt" title="Timestep" dataType="xsd:double" unitType="unitType:time" units="nonSi:picoseconds">
		<cml:definition>
			<xhtml:p>
				Timestep that should be used to solve the equations of movement.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
				Timestep that should be used in the integrator algorithms to solve the
				equations of movement.
			</xhtml:p>
		</cml:description>
	</cml:entry>
	
	<cml:entry id="grompp.NeighbourSearchingAlgorithm" term="ns_type" title="Algorithm to use for constructing Neighbour List" dataType="xsd:string" unitType="unitType:none" units="si:none">      
		<cml:definition> 
			<xhtml:p>
				The algorithm employed to perform the update of the neighbour list.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
			The intermolecular interactions will be calculated only between atoms that can 'see' each other. To determine if two atoms can interact with each other a neighbour search is performed.
			</xhtml:p>
		</cml:description>
		<cmlx:restriction id="NSA.Grid" title="Grid">grid</cmlx:restriction>
		<cmlx:restriction id="NSA.Simple" title="Simple">Simple</cmlx:restriction>
	</cml:entry>
      
	<cml:entry id="grompp.NSA.updateFrequency" term="nstlist" title="Update Frequency for Neighbour List construction" dataType="xsd:integer" unitType="unitType:none" units="si:none" >
		<cml:definition>
			<xhtml:p>
			The number of steps that should pass before the neigbourlist is updated.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
			The number of steps that should pass before the neigbourlist will be updated. A value of zero means the neighbour list will only be constructed once. A negative value should be interpreted as an automatically chosen update frequency.       
			</xhtml:p>
		</cml:description>
	</cml:entry>
		
	<cml:entry id="grompp.NSA.radius" term="rlist" title="Cut-Off radius" dataType="xsd:double" unitType="unitType:length" units="nonSi:nanometers">
		<cml:definition>
			<xhtml:p>
				Cut-Off radius around an atom in which another atom is counted as a
				neighbour.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
				Only atoms within this radius around an atom will be counted as
				neighbours.
			</xhtml:p>
		</cml:description>
	</cml:entry>
	
	
	<cml:entry id="grompp.NSA.PBC" term="pbc" title="Handling of Periodic Boundary Conditions in Neighbour Searching" dataType="xsd:string" unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
			The way periodic boundary conditions will be handled in Neighbour Searching.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
			The options that will be used for periodic boundary conditions in neighbour searching.
			</xhtml:p>
		</cml:description>
		<cmlx:restriction id="NSA.PBC.xyz" title="3D PBC">xyz</cmlx:restriction>
		<cmlx:restriction id="NSA.PBC.xy" title="2D PBC">xy</cmlx:restriction>
		<cmlx:restriction id="NSA.PBC.none" title="No PBC">no</cmlx:restriction>
	</cml:entry>
     
	<cml:entry id="grompp.PressureCouplingAlgorithm" term="pcoupl" title="Pressure Coupling Algorithm" dataType="xsd:string" unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
			The algorithm applied to emulate the barostat in the system.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
			One can achieve keeping the pressure constant in a MD simulation setup by using different algorithms. By using this cml:entry you can specify the barostatic algorithm to impose the pressure constraints on the system.
			</xhtml:p>
		</cml:description>
		<cmlx:restriction id="PCA.none" title="none">none</cmlx:restriction>
		<cmlx:restriction id="PCA.Berendsen" title="Berendsen">berendsen</cmlx:restriction>
		<cmlx:restriction id="PCA.Parrinello-Rahman" title="Parrinello-Rahman">Parrinello-Rahman</cmlx:restriction>
		<cmlx:restriction id="PCA.MTTK" title="Martyna-Tuckerman-Tobias-Klein">MTTK</cmlx:restriction>
	</cml:entry>
    
	<cml:entry id="grompp.PCA.pressure" term="ref_p" title="Pressure in Pressure Coupling Algorithm" dataType="xsd:double" unitType="unitType:pressure" units="nonSi:bar">
		<cml:definition>
			<xhtml:p>            
			Pressure in bar imposed on the system.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>            
			Pressure in bar imposed on the system.
			</xhtml:p>
		</cml:description>
	</cml:entry>
 
	<cml:entry id="grompp.PCA.exchangeInterval" term="tau_p" title="Exchange Interval" dataType="xsd:double" unitType="unitType:time" units="nonSi:picoseconds">
		<cml:definition>
			<xhtml:p>            
			Interval in ps in which the system should be equilibrated with the barostat.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
			Interval in ps in which the system should be equilibrated with the barostat.
			</xhtml:p>
		</cml:description>
	</cml:entry>

	<cml:entry id="grompp.PCA.compressibility" term="compressibility" title="Compressibility" dataType="xsd:double" unitType="unitType:pressure" units="nonSi:bar">
		<cml:definition>
			<xhtml:p>            
			Compressibility for pressure coupling (NOTE: this is now really in [1/bar]). For water at 1 atm and 300 K the compressibility is 4.5e-5 [1/bar].
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>            
			Compressibility for pressure coupling (NOTE: this is now really in [1/bar]). For water at 1 atm and 300 K the compressibility is 4.5e-5 [1/bar].
			</xhtml:p>
		</cml:description>
	</cml:entry>
		
	<cml:entry id="grompp.PCA.couplingType" term="pcoupltype" title="Coupling Type" dataType="xsd:string" unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
			Pressure coupling type that is used to spread the pressure constraints through the system.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
			Pressure coupling type that is used to spread the pressure constraints through the system.
			</xhtml:p>
		</cml:description>
	<cmlx:restriction id="PCA.couplT.None" title="None">none</cmlx:restriction>
	<cmlx:restriction id="PCA.couplT.Isotropic" title="Isotropic">isotropic</cmlx:restriction>
	<cmlx:restriction id="PCA.couplT.SemiIsotropic" title="Semiisotropic">semiisotropic</cmlx:restriction>
	<cmlx:restriction id="PCA.couplT.Anisotropic" title="Anisotropic">anisotropic</cmlx:restriction>
	<cmlx:restriction id="PCA.couplT.SurfaceTension" title="Surface Tension">surface-tension</cmlx:restriction>
	</cml:entry>

	<cml:entry id="grompp.ConstraintAlgorithm" term="constraint-algorithm" title="Constraint Algorithm" dataType="xsd:string" unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
			The algorithm used to constrain bonds or angles in a molecule.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
			For certain types of simulations, e.g. iterative relaxation of a protein,
			or to increase the integration time step it can be reasonable or necessary
			to keep bond angles or bond lengths fixed. Different algorithms are 
			available to perform this task.
			</xhtml:p>
		</cml:description>
		<cmlx:restriction id="CA.LINCS" title="LINCS">LINCS</cmlx:restriction>
		<cmlx:restriction id="CA.SHAKE" title="SHAKE">SHAKE</cmlx:restriction>
	</cml:entry>
	
	<cml:entry id="grompp.Constraints" term="constraints" title="Constraints" dataType="xsd:string" unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
			Decides where constraints are applied.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
			Decides where constraints are applied.
			</xhtml:p>
		</cml:description>
		<cmlx:restriction id="Constraints.None" title="None">none</cmlx:restriction>
		<cmlx:restriction id="Constraints.HBonds" title="Hydrogen Bonds">hbonds</cmlx:restriction>
		<cmlx:restriction id="Constraints.All" title="All Bonds">all-bonds</cmlx:restriction>
		<cmlx:restriction id="Constraints.HAngles" title="Hydrogen Angles">h-angles</cmlx:restriction>
		<cmlx:restriction id="Constraints.AllAngles" title="All Angles">all-angles</cmlx:restriction>
	</cml:entry>
	
	<cml:entry id="grompp.lincs.order" term="lincs-order" title="LINCS Order" dataType="xsd:int" unitType="unitType:none" units="si:none">
	<cml:definition>
		<xhtml:p>
		Highest order in the expansion of the constraint coupling matrix.
		</xhtml:p>
	</cml:definition>
	<cml:description>
		<xhtml:p>
		Highest order in the expansion of the constraint coupling matrix. 
		</xhtml:p>
	</cml:description>
	</cml:entry>

	<cml:entry id="grompp.lincs.iter" term="lincs_iter" title="LINCS Iterations" dataType="xsd:int" unitType="unitType:none" units="si:none">
	<cml:definition>
		<xhtml:p>
		Number of iterations to correct for rotational lengthening in LINCS.
		</xhtml:p>
	</cml:definition>
	<cml:description>
		<xhtml:p>
		Number of iterations to correct for rotational lengthening in LINCS.
		</xhtml:p>
	</cml:description>
	</cml:entry>

	<cml:entry id="grompp.CA.type" term="CA.type" dataType="xsd:string" unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
				Defines to which bonds and angles the constraints are applied.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
            The constraints can be applied to certain categories of intramolecular
            potential, i.e. hydrogen bonds, hydrogen angles, all bonds, all angles.
			</xhtml:p>
		</cml:description>
		<cmlx:restriction id="CA.type.none" title="none">none</cmlx:restriction>
		<cmlx:restriction id="CA.type.HydrogenBonds" title="Hydrogen Bonds">hbonds</cmlx:restriction>
		<cmlx:restriction id="CA.type.AllBonds" title="All Bonds">all-bonds</cmlx:restriction>
		<cmlx:restriction id="CA.type.HydrogenAngles" title="Hydrogen Angles">h-angles</cmlx:restriction>
		<cmlx:restriction id="CA.type.AllAngles" title="All Angles">all-angles</cmlx:restriction>
	</cml:entry>
  
	<cml:entry id="grompp.TemperatureCouplingAlgorithm" term="tcoupl" title="Temperature Coupling Algorithm" dataType="xsd:string" unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
			The algorithm applied to emulate the thermostat in the system.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
				One can achieve keeping the temperature constant in a MD simulation setup by using different algorithms. By using this cml:entry you can specify the thermostatic algorithm to impose the temperature constraints on the system.
			</xhtml:p>
		</cml:description>
	<cmlx:restriction id="TCA.none" title="none">none</cmlx:restriction>
	<cmlx:restriction id="TCA.Berendsen" title="Berendsen">berendsen</cmlx:restriction>
	<cmlx:restriction id="TCA.Noose-Hoover" title="Noose-Hoover">Noose-Hoover</cmlx:restriction>
	<cmlx:restriction id="TCA.V-rescale" title="Velocity rescale algorithm">V-rescale</cmlx:restriction>
	</cml:entry>
    
	<cml:entry id="grompp.TCA.couplingGroup" term="tc-grps" title="Coupling Groups" dataType="xsd:string" unitType="unitType:none" units="si:none">
	<cml:definition>
		<xhtml:p>
		Name of the coupling group to which the temperature coupling should be applied.
		</xhtml:p>
	</cml:definition>
	<cml:description>
		<xhtml:p>
		Some programs support that one can define different coupling groups to which different settings for the temperature coupling can be applied.
		</xhtml:p>
	</cml:description>
	</cml:entry>
	
	<cml:entry id="grompp.TCA.temperature" term="ref_t" title="Temperature for Temperature Coupling Algorithm" dataType="xsd:string" unitType="unitType:none" units="si:none">
	<cml:definition>
		<xhtml:p>
		Temperature that should be applied by thermostat algorithm.
		</xhtml:p>
	</cml:definition>
	<cml:description>
		<xhtml:p>
		Temperature that should be applied by the thermostat algorithm.
		</xhtml:p>
	</cml:description>
	</cml:entry>
    
	<cml:entry id="grompp.TCA.exchangeInterval" term="tau_t" title="Exchange Interval in Temperature Coupling Algorithm" dataType="xsd:string" unitType="unitType:none" units="si:none">
	<cml:definition>
		<xhtml:p>            
		Interval in ps in which the system should be equilibrated with the thermostat.
		</xhtml:p>
	</cml:definition>
	<cml:description>
		<xhtml:p>
		Interval in ps in which the system should be equilibrated with the thermostat.
		</xhtml:p>
	</cml:description>
	</cml:entry>  

	<cml:entry id="grompp.ElectrostaticsAlgorithm" term="coulombtype" title="Electrostatic algorithm" dataType="xsd:string" unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
				Algorithm that is used to calculate the electrostatics and coulumbic
				interactions, respectively.
			</xhtml:p>
		</cml:definition>
		<cml:description>
