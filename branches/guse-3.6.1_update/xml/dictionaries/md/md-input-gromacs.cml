	<cml:entry id="pdb2gmx.Forcefield" term="-ff" title="Force Field" dataType="xsd:string"
		unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
				The name of the forcefield applied to the molecular system without the
				program and the version.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
				The force field describes how atoms, bonds, bond angles and dihedrals
				should be treated.
			</xhtml:p>
		</cml:description>
		<cmlx:restriction id="md.forcefield.oplsaa" title="OPLS-AA">oplsaa</cmlx:restriction>
		<cmlx:restriction id="md.forcefield.G43a1" title="G43a1">gromos43a1</cmlx:restriction>
		<cmlx:restriction id="md.forcefield.G43a2" title="G43a2">gromos43a2</cmlx:restriction>
		<cmlx:restriction id="md.forcefield.G45a3" title="G45a3">gromos45a3</cmlx:restriction>
		<cmlx:restriction id="md.forcefield.G53a5" title="G53a5">gromos53a5</cmlx:restriction>
		<cmlx:restriction id="md.forcefield.G53a6" title="G53a6">gromos53a6</cmlx:restriction>
		<!-- <cmlx:restriction id="md.forcefield.G43b1" title="G43b1">G43b1</cmlx:restriction> -->
		<cmlx:restriction id="md.forcefield.gmx" title="Gmx">gmx</cmlx:restriction>
		<cmlx:restriction id="md.forcefield.encads" title="Encads">encads</cmlx:restriction>
		<cmlx:restriction id="md.forcefield.encadv" title="Encadv">encadv</cmlx:restriction>
	</cml:entry>
	
	<cml:entry id="pdb2gmx.IgnoreHydrogens" term="-ignh" title="Ignore Hydgrogens" dataType="xsd:string" cmlx:flag="true" unitType="unitType:none" units="si:none">
		<cml:definition>
			<xhtml:p>
				Ignore hydrogen atoms that are in the input file. Deleted hydrogens will be readded under force field specific rules.
			</xhtml:p>
		</cml:definition>
		<cml:description>
			<xhtml:p>
				Ignore hydrogen atoms that are in the input file. Deleted hydrogens will be readded under force field specific rules. This option is strongly recommended.
			</xhtml:p>
		</cml:description>
	</cml:entry>

